{
    "name": "chromeshack",
    "version": "1.6.9",
    "description": "Collection of scripts for Shacknews.",
    "repository": "https://github.com/latestchatty/chromeshack",
    "scripts": {
        "extlint": "npx web-ext lint -s ./dist",
        "webext": "npx web-ext run -s dist -u https://shacknews.com/chatty",
        "lint": "npx eslint ./src/**/*.{ts,tsx}",
        "lint-fix": "npx eslint --fix ./src/**/*.{ts,tsx}",
        "zip": "npx web-ext build -s ./dist -a ./artifacts -o",
        "sign": "npx web-ext sign -s ./dist -a ./artifacts",
        "pack": "node ./utils/packsrc.js",
        "build:dev": "npx webpack-cli --config ./utils/webpack.config.dev.js --watch --info-verbosity verbose",
        "build:prod": "npx webpack-cli --config ./utils/webpack.config.prod.js --info-verbosity verbose",
        "build:pack": "npm run build:prod && npm run zip && npm run pack"
    },
    "author": "Adam Hughes",
    "email": "arhughes@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.4",
        "@types/chrome": "0.0.122",
        "@types/dompurify": "^2.0.4",
        "@types/jquery": "^3.5.1",
        "@types/luxon": "^1.24.4",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "archiver": "^5.0.2",
        "css-loader": "^4.3.0",
        "eslint": "^7.9.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.2",
        "eslint-plugin-sonarjs": "^0.5.0",
        "filemanager-webpack-plugin": "^2.0.5",
        "fork-ts-checker-webpack-plugin": "^5.2.0",
        "license-checker-webpack-plugin": "^0.1.5",
        "mini-css-extract-plugin": "^0.11.2",
        "prettier": "^2.1.1",
        "terser": "^5.3.1",
        "terser-webpack-plugin": "^4.2.0",
        "ts-loader": "^8.0.3",
        "typescript": "^4.0.2",
        "web-ext": "^5.0.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-extension-reloader": "^1.1.4",
        "webpack-merge": "^5.1.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-regular-svg-icons": "^5.14.0",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "dompurify": "^2.0.15",
        "embla-carousel": "^4.0.3",
        "jquery": "^3.5.1",
        "luxon": "^1.25.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "text-field-edit": "^3.1.0",
        "ts-debounce": "^2.0.1",
        "webextension-polyfill-ts": "^0.19.0"
    }
}
