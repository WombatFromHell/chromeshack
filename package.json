{
  "name": "chromeshack",
  "version": "1.74.0",
  "description": "Collection of scripts for Shacknews.",
  "repository": "https://github.com/latestchatty/chromeshack",
  "type": "module",
  "scripts": {
    "generate-licenses": "node utils/generateLicenses.js",
    "generate-cookie": "node utils/testlogin.js",
    "pkgsrc": "node utils/package.js",
    "build:serviceWorker": "vite build --config vite.serviceWorker.config.ts",
    "build:content": "vite build --config vite.content.config.ts",
    "build:popup": "vite build --config vite.popup.config.ts",
    "build:js": "run-s build:serviceWorker build:content build:popup",
    "clean": "rm -rf dist/",
    "cleanall": "rm -rf dist/ dist-firefox/ dist-chrome/",
    "mvsrc:chrome": "rm -rf dist-chrome/ && mv -f dist dist-chrome",
    "mvsrc:firefox": "rm -rf dist-firefox/ && mv -f dist dist-firefox",
    "manifest:clean": "rm -f dist/manifest.sm.json dist/manifestv3.ffx.json",
    "manifest:dev": "mv dist/manifest.sm.json dist/manifest.json && rm -f dist/manifestv3.ffx.json",
    "manifest:firefox": "mv dist/manifestv3.ffx.json dist/manifest.json && rm -f dist/manifest.sm.json",
    "dev": "tsc && NODE_ENV=development run-s clean build:js manifest:dev mvsrc:chrome",
    "dev:firefox": "tsc && NODE_ENV=development run-s clean build:js manifest:firefox mvsrc:firefox",
    "dev:all": "run-s cleanall dev dev:firefox",
    "build": "tsc && NODE_ENV=production run-s clean build:js manifest:clean mvsrc:chrome",
    "build:firefox": "tsc && NODE_ENV=production run-s clean build:js manifest:firefox mvsrc:firefox",
    "build:pkg": "run-s cleanall build build:firefox pkgsrc",
    "test": "playwright test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "dompurify": "^3.0.6",
    "embla-carousel-react": "^8.0.0-rc14",
    "html-react-parser": "^4.2.2",
    "jquery": "^3.7.1",
    "lz-string": "^1.5.0",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "text-field-edit": "^4.0.0",
    "ts-debounce": "^4.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@playwright/test": "^1.39.0",
    "@types/chrome": "^0.0.246",
    "@types/dompurify": "^3.0.3",
    "@types/jquery": "^3.5.22",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "@types/webextension-polyfill": "^0.10.4",
    "@vitejs/plugin-react": "^4.1.0",
    "adm-zip": "^0.5.10",
    "dotenv": "^16.3.1",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
}
